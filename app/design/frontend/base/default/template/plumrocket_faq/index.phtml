<?php
$faqs = $this->getFaqsCollection();
$faqsViewUrl = Mage::getUrl('faqs/index/view');
?>
<?php if (Mage::getStoreConfig('faq_section/settings/enable')) :?>
<?php foreach ($faqs as $item): ?>
    <h2>
        <a href="<?php echo $faqsViewUrl; ?>?id=<?php echo $item->getId(); ?>">
            <?php echo $item->getTitle(); ?>
        </a>
    </h2>
<?php endforeach; ?>
<?php endif; ?>